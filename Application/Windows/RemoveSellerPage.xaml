        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Application.Windows"
        mc:Ignorable="d"
        Title="RemoveSellerPage" Height="185" Width="295">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
